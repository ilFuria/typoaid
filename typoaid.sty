	\_typoAid_calcAll:n{#2}
	\dim_set:Nn\l__typoAid_dummy_dim{\fp_to_dim:n{\dim_to_fp:n{\l__typoAid_MeanChar_dim}*\int_eval:n{#3}}}
	\IfBooleanTF{#1}{%
	\msg_term:n{\c__typoAid_name_string;~for:\tl_to_str:n{#2}\ %
	\iow_newline:*~for~\int_eval:n{#3}~char,\iow_newline:*~ %
	colwidth:~\dim_eval:n{\l__typoAid_dummy_dim}~(\dim_to_decimal_in_unit:nn{\l__typoAid_dummy_dim}{1pc}~pc)}
	}{%
		In~order~to~obtain~the~required~\int_eval:n{#3}~characters~per~column,~it~should~have~a~width~of~
	\dim_eval:n{\l__typoAid_dummy_dim}~(\dim_to_decimal_in_unit:nn{\l__typoAid_dummy_dim}{1pc}~pc)
